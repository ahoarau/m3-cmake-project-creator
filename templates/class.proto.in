option optimize_for = SPEED;
// Import here all other proto files you need
import "component_base.proto";


message @CLASS_NAME@Status
{
    optional M3BaseStatus    	base=1;                        //reserved
    optional double        	foo=2;                        //sensor value for ex
}

message @CLASS_NAME@Param
{
    optional double            	gain=1;                //Controller gain for ex
}

message @CLASS_NAME@Command
{
    optional bool            	enable=1;                //turn  on/off
}
